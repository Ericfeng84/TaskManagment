{
  "name": "personal-task-manager",
  "version": "1.0.0",
  "description": "Personal Task Management Assistant",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd spring-boot-server && mvn spring-boot:run",
